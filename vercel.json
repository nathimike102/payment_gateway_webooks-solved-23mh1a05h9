{
  "version": 2,
  "public": "frontend/dist",
  "buildCommand": "echo 'no build'",
  "outputDirectory": "frontend/dist",
  "routes": [
    {
      "src": "/api/v1/(.*)",
      "dest": "/api/v1.js"
    },
    {
      "src": "/assets/(.*)",
      "dest": "/assets/$1"
    },
    {
      "src": "/.*",
      "dest": "/index.html"
    }
  ]
}